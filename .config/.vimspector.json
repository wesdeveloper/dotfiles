{
  "configurations": {
    "attach":{
      "adapter": "vscode-node",
      "default": true,
      "configuration": {
        "name": "Attach to Process",
        "type": "node",
        "request": "attach",
        "skipFiles": ["<node_internals>/**/*.js"],
        "processId": "${processId}"
      }
    }
  }
}
